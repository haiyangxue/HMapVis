<div class="wrapper">
	<div class="title">
		<h1>添加历史地图</h1>
	</div>			
	<div class="add-content">
		<img src="images/loading.gif" id="loading" style="display: none;"></img>
		<input type="file" id="file" name="file" /><br />
		 
		<input type="button" id="upload" value="上传" class="buttonInput" style="width:100%;" /><br/>
		<input type="button" id="reupload" value="重新上传" class="buttonInput" style="width:100%;display: none;" /><br/>
		<input type="button" id="showMap" value="在地图上显示查看效果" class="buttonInput" style="width:100%;" onclick="showMap();" />
		<input type="button" id="editMap" value="重新编辑" class="buttonInput" style="width:100%;display:none;" onclick="editMap();" /><br/><br/>
		
		<div class="box">
			地图名称：<input type="text" id="map_name" class="txtInput" style="width:100%;"></input>
		</div><br/>
		<div class="box" id="dl">
		
		</div><br/>
		<div class="box" id="tl">
			所属类型：
			<select id="mapType" class="txtInput" style="width:100%;">
				<option value = "1">国家级</option>
				<option value = "2">省级</option>
			</select>
		<div><br />
		<div class="box">
			<input type="text" id="map_start" class="txtInput" value="开始时间" onfocus="onTimeStartfocus('map_start')" onblur="onTimeStartBlur('map_start')" style="float:left; width:49%;"></input>
			<span style="float:left; margin-top:12px; width:2%;">&nbsp-&nbsp</span>
			<input type="text" id="map_end" class="txtInput" value="结束时间" onfocus="onTimeEndfocus('map_end')" onblur="onTimeEndBlur('map_end')" style="float:left; width:49%;"></input><br />
		</div>
		<input type="button" id="map_submit" value="提交" class="buttonInput" style="width:100%;" /><br/>
		<input type="button" id="map_cancel" value="取消" class="buttonInput" style="width:100%;" /><br/><br/>
	</div>
</div>