<div class="wrapper">
	<div class="title">
		<h1>编辑历史事件</h1>
	</div>
	<div class="add-content">
		<div class="box">
			事件名称：<input value = "{{event_name}}" type="text" id="event_name" class="txtInput" style="width:100%;"></input>
		</div>	
		<div class="box" id="et">

		</div>
		<div class="box" id="dl">
	
		</div>
		<div class="box" id="pl">
			发生地点：<input type="text" id="tem" class="txtInput" style="width:100%;"  readonly="readonly" value="请先选择朝代"></input>
		</div>

		<div class="box">
			时间范围：<br /> 
			<input value = "{{start_date}}" type="text" id="place_start" class="txtInput" value="开始时间" onfocus="onTimeStartfocus('place_start')" onblur="onTimeStartBlur('place_start')" style="float:left; width:49%;"></input> 
			<span style="float:left; margin-top:12px; width:2%;">&nbsp-&nbsp</span> 
			<input value = "{{end_date}}" type="text" id="place_end" class="txtInput" value="结束时间" onfocus="onTimeEndfocus('place_end')" onblur="onTimeEndBlur('place_end')" style="float:left; width:49%;"></input><br />
		</div>
		<div class="box">
			参与人物：
			<input value = "{{people}}" type="text" id="event_people" class="txtInput" style="width:100%;"></input>
		</div>
		<div class="box">
			事件影响：
			<input value = "{{influ}}" type="text" id="event_infu" class="txtInput" style="width:100%;"></input>
		</div>
		<div class="box">
			基本信息：
			<input value = "{{summary}}" type="text" id="event_summary" class="txtInput" style="width:100%;"></input>
		</div>
		<div clas="box">
			详细介绍：
			<input value = "{{detail_url}}" type="text" id="event_detail" class="txtInput" style="width:100%;"></input>
		</div>
		<div clas="box">
			相关视频：
			<input value = "{{event_video}}" type="text" id="event_video" class="txtInput" style="width:100%;"></input>
		</div>
		<input type="button" id="event_submit" class="buttonInput" style="margin-top:10px; width:100%;" value="提交"></input>
		<input type="button" id="event_cancel" class="buttonInput" style="margin-top:10px; width:100%;" value="取消"></input>
	</div>
</div>